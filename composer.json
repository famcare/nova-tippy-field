{
    "name": "monaye/nova-tippy-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Monaye\\NovaTippyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monaye\\NovaTippyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
